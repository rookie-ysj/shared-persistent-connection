  "repository": "https://github.com/Monkey-D-sj/shared-persistent-connection",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "share persistent connection",
    "sse"
  ],
  "types": "./dist/index.d.ts",
